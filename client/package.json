{
  "name": "dumber-gist",
  "description": "Dumber gist client side app",
  "keywords": [],
  "private": true,
  "license": "MIT",
  "devDependencies": {
    "@aurelia/plugin-conventions": "latest",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "aurelia-bootstrapper": "^2.4.1",
    "aurelia-combo": "^1.1.4",
    "aurelia-deps-finder": "^2.1.7",
    "aurelia-dialog-lite": "^1.0.1",
    "aurelia-testing": "^1.1.0",
    "base64-arraybuffer": "^1.0.2",
    "bcx-aurelia-dnd": "^1.6.0",
    "bcx-aurelia-reorderable-repeat": "^1.5.1",
    "bcx-validation": "^2.1.0",
    "browser-do": "^5.0.0",
    "clipboard": "^2.0.11",
    "codemirror": "^5.65.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "esm": "^3.2.25",
    "graphlib": "^2.1.8",
    "gulp": "^4.0.2",
    "gulp-dart-sass": "^1.1.0",
    "gulp-dumber": "^3.0.0",
    "gulp-if": "^3.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-swc": "^2.2.0",
    "gulp-terser": "^2.1.0",
    "htmlhint": "^1.1.4",
    "is-utf8": "^0.2.1",
    "less": "^4.2.1",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "merge2": "^1.4.1",
    "moment": "^2.30.1",
    "nyc": "^15.1.0",
    "sass": "~1.49.11",
    "sass-lint": "^1.13.1",
    "sass.js": "^0.11.1",
    "semver": "^7.6.3",
    "source-map": "^0.8.0-beta.0",
    "svelte": "^5.16.0",
    "ts-plugin-inferno": "^6.1.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "zora": "^6.0.0"
  },
  "overrides": {
    "chokidar": "^3.5.3",
    "glob-stream": "^7.0.0",
    "glob-parent": "^6.0.2",
    "micromatch": "^4.0.5"
  },
  "scripts": {
    "css-lint": "sass-lint -c .sass-lint.yml 'src/**/*.scss'",
    "js-lint": "eslint src test src-worker test-worker",
    "html-lint": "htmlhint -c .htmlhintrc src",
    "lint": "npm run js-lint && npm run css-lint && npm run html-lint",
    "coverage": "npm run test && nyc report --reporter=lcov --reporter=text",
    "start": "cross-env NODE_OPTIONS=--max_old_space_size=3584 gulp",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=3584 gulp build",
    "build:prod": "cross-env NODE_ENV=production NODE_OPTIONS=--max_old_space_size=3584 gulp build",
    "clear-cache": "gulp clear-cache",
    "pretest": "npm run lint && npm run build:test",
    "build:test": "cross-env NODE_ENV=test NODE_OPTIONS=--max_old_space_size=3584 gulp build",
    "test": "npm run test:app && npm run test:worker",
    "test:app": "browser-do --tap < dist/entry-bundle.js",
    "browser-test:app": "npm run build:test && browser-do --tap --browser chrome < dist/entry-bundle.js",
    "test:worker": "browser-do --tap < dist/bundler-worker.js",
    "browser-test:worker": "npm run build:test && browser-do --tap --browser chrome < dist/bundler-worker.js"
  }
}
