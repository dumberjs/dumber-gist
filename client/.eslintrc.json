{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": 0,
    "@typescript-eslint/no-empty-function": 0,
    "no-console": 0,
    "getter-return": 0,
    "no-prototype-builtins": 0
  },
  "env": {
    "es2017": true,
    "browser": true,
    "node": true,
    "worker": true
  },
  "globals": {
    "requirejs": true,
    "HOST_NAMES": true,
    "DUMBER_MODULE_LOADER_DIST": true
  }
}
