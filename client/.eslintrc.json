{
  "root": true,
  "extends": "eslint:recommended",
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "legacyDecorators": true
    }
  },
  "rules": {
    "no-console": 0,
    "getter-return": 0,
    "no-prototype-builtins": 0
  },
  "env": {
    "es2017": true,
    "browser": true,
    "node": true,
    "worker": true
  },
  "globals": {
    "requirejs": true,
    "HOST_NAMES": true,
    "DUMBER_MODULE_LOADER_DIST": true
  }
}
